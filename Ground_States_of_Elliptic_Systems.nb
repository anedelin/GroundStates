Cell[TextData[{
 "Further we run some crosschecks of our results. First check is identity \
ground state wavefunction should satisfy is the behavior w.r.t. ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "->", 
    SuperscriptBox["t", 
     RowBox[{"-", "1"}]]}], TraditionalForm]],ExpressionUUID->
  "60a6331f-a3ea-4c5b-9bd8-ef3028f2cfda"],
 " reflection\n                                                               \
                                                            ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["\[Psi]", "0"], "(", "x", ")"}], 
     SubscriptBox["|", 
      RowBox[{"t", "->", 
       SuperscriptBox["t", 
        RowBox[{"-", "1"}]]}]]}], "=", 
    RowBox[{
     RowBox[{"\[CapitalGamma]", "(", 
      FractionBox[
       RowBox[{
        RowBox[{"\[Sqrt]", "pq"}], " ", 
        SuperscriptBox["x", "2"]}], "t"], ")"}], 
     RowBox[{"\[CapitalGamma]", "(", 
      FractionBox[
       RowBox[{
        RowBox[{"\[Sqrt]", "pq"}], " ", 
        SuperscriptBox["x", 
         RowBox[{"-", "2"}]]}], "t"], ")"}], 
     SuperscriptBox[
      RowBox[{"\[CapitalGamma]", "(", 
       FractionBox[
        RowBox[{"\[Sqrt]", "pq", " "}], "t"], ")"}], "4"], 
     RowBox[{
      SubscriptBox["\[Psi]", "0"], "(", "x", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"2730c7fa-203f-4199-a099-bb7129120fdb"]
}], "Text",
