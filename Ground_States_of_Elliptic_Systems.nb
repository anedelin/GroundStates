       RowBox[{"GPl", "[", 
        RowBox[{
         RowBox[{"x", " ", 
          SuperscriptBox["t", 
           RowBox[{
            RowBox[{"-", "1"}], "/", "6"}]], " ", 
          RowBox[{"tt", "[", 
           RowBox[{"[", "i", "]"}], "]"}], " ", 
          SuperscriptBox["y2", 
           RowBox[{"-", "1"}]]}], ",", "x", ",", "n"}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "3"}], "}"}]}], "]"}], "+", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"GPl", "[", 
        RowBox[{
         RowBox[{"x", " ", 
          SuperscriptBox["t", 
           RowBox[{
            RowBox[{"-", "1"}], "/", "6"}]], " ", 
