Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"trinion", "[", 
    RowBox[{"a_", ",", "b_", ",", "c_", ",", "t_", ",", "x_", ",", "n_"}], 
    "]"}], "=", 
   RowBox[{
    RowBox[{"GPl", "[", 
     RowBox[{
      RowBox[{"x", " ", "t"}], ",", "x", ",", "n"}], "]"}], 
    RowBox[{"(", 
     RowBox[{"a", "+", 
      FractionBox["1", "a"]}], ")"}], 
    RowBox[{"(", 
     RowBox[{"b", "+", 
      FractionBox["1", "b"]}], ")"}], 
    RowBox[{"(", 
     RowBox[{"c", "+", 
      FractionBox["1", "c"]}], ")"}]}]}], ";"}]], "Input",
