Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Trinion", "[", 
    RowBox[{"y1_", ",", "y2_", ",", "y3_", ",", "t_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"Series", "[", 
     RowBox[{
      RowBox[{"Coefficient", "[", 
