Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"genus1", "[", 
    RowBox[{"u_", ",", "v_", ",", "t_"}], "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"Series", "[", 
      RowBox[{
       RowBox[{"Coefficient", "[", 
        RowBox[{
         RowBox[{"Coefficient", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"vand", "[", "a", "]"}], 
            RowBox[{"vand", "[", "b", "]"}], 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"Series", "[", 
               RowBox[{
                SuperscriptBox["\[ExponentialE]", 
                 RowBox[{
                  UnderoverscriptBox["\[Sum]", 
                   RowBox[{"l", "=", "1"}], 
                   RowBox[{"2", "M"}]], 
                  RowBox[{
                   FractionBox["1", "l"], 
                   RowBox[{"(", 
                    RowBox[{
                    RowBox[{"trinion", "[", 
                    RowBox[{
                    SuperscriptBox["u", "l"], ",", 
                    SuperscriptBox["a", "l"], ",", 
                    SuperscriptBox["b", "l"], ",", 
                    SuperscriptBox["t", "l"], ",", 
                    SuperscriptBox["x", "l"], ",", 
                    SuperscriptBox["n", "l"]}], "]"}], "+", 
                    RowBox[{"trinion", "[", 
                    RowBox[{
                    SuperscriptBox["v", "l"], ",", 
                    SuperscriptBox["a", "l"], ",", 
                    SuperscriptBox["b", "l"], ",", 
                    SuperscriptBox["t", 
                    RowBox[{"-", "l"}]], ",", 
                    SuperscriptBox["x", "l"], ",", 
                    SuperscriptBox["n", "l"]}], "]"}], "+", 
                    RowBox[{"sglue", "[", 
                    RowBox[{
                    SuperscriptBox["a", "l"], ",", 
                    SuperscriptBox["x", "l"], ",", 
                    SuperscriptBox["n", "l"]}], "]"}], "+", 
                    RowBox[{"sglue", "[", 
                    RowBox[{
                    SuperscriptBox["b", "l"], ",", 
                    SuperscriptBox["x", "l"], ",", 
                    SuperscriptBox["n", "l"]}], "]"}]}], ")"}]}]}]], ",", 
                RowBox[{"{", 
                 RowBox[{"x", ",", "0", ",", "M"}], "}"}]}], "]"}], "//", 
              "Normal"}], ")"}]}], ",", "a", ",", "0"}], "]"}], ",", "b", ",",
          "0"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "0", ",", "M"}], "}"}]}], "]"}], "//", "Simplify"}],
     "//", "Normal"}]}], ";"}]], "Input",
