          RowBox[{"tt", "[", 
           RowBox[{"[", "i", "]"}], "]"}], " ", "y1"}], ",", "x", ",", "n"}], 
        "]"}], "+", 
       RowBox[{"GPl", "[", 
        RowBox[{
         RowBox[{"x", " ", 
          SuperscriptBox["t", 
           RowBox[{"4", "/", "3"}]], " ", 
          RowBox[{"tt", "[", 
           RowBox[{"[", "i", "]"}], "]"}], " ", 
          SuperscriptBox["y1", 
           RowBox[{"-", "1"}]]}], ",", "x", ",", "n"}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "3"}], "}"}]}], "]"}], "+", 
