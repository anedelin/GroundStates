      SubscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"q", " ", 
          SuperscriptBox["x", 
           RowBox[{"-", "2"}]]}], ";", "q"}], ")"}], "\[Infinity]"], 
      SubscriptBox[
       RowBox[{"(", 
