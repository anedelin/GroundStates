Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"tube2", "[", 
    RowBox[{"a_", ",", "b_"}], "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"Series", "[", 
      RowBox[{
