         SuperscriptBox["Y", 
          RowBox[{"-", "3"}]]}]}], ")"}]}], 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Theta]", "p"], "(", 
       RowBox[{
        SuperscriptBox["p", 
         RowBox[{"1", "/", "2"}]], "Y"}], ")"}], 
      RowBox[{
       SubscriptBox["\[Theta]", "p"], "(", 
       RowBox[{
        SuperscriptBox["p", 
         RowBox[{"1", "/", "2"}]], 
        SuperscriptBox["Y", 
         RowBox[{"-", "1"}]]}], ")"}]}]]}], TraditionalForm]],ExpressionUUID->
  "a4639aef-7ca2-4134-a84f-3926730374c4"],
