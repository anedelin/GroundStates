        RowBox[{"Coefficient", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"VandSU3", "[", 
            RowBox[{"t1", ",", "t2"}], "]"}], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Series", "[", 
              RowBox[{
               RowBox[{"Exp", "[", 
                RowBox[{
                 UnderoverscriptBox["\[Sum]", 
                  RowBox[{"l", "=", "1"}], 
                  RowBox[{"2", "M"}]], 
                 RowBox[{
                  FractionBox["1", "l"], 
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"VecSU3", "[", 
                    RowBox[{
                    SuperscriptBox["t1", "l"], ",", 
                    SuperscriptBox["t2", "l"], ",", 
                    SuperscriptBox["x", "l"], ",", 
                    SuperscriptBox["n", "l"]}], "]"}], "+", 
                    RowBox[{"TrinionIntegrand", "[", 
                    RowBox[{
                    SuperscriptBox["y1", "l"], ",", 
                    SuperscriptBox["y2", "l"], ",", 
                    SuperscriptBox["y3", "l"], ",", 
                    SuperscriptBox["t", "l"], ",", 
                    SuperscriptBox["x", "l"], ",", 
                    SuperscriptBox["n", "l"], ",", 
                    SuperscriptBox["t1", "l"], ",", 
                    SuperscriptBox["t2", "l"], ",", 
                    FractionBox["1", 
                    RowBox[{
                    SuperscriptBox["t1", "l"], 
                    SuperscriptBox["t2", "l"]}]]}], "]"}]}], ")"}]}]}], "]"}],
                ",", 
               RowBox[{"{", 
                RowBox[{"x", ",", "0", ",", "M"}], "}"}]}], "]"}], "//", 
             "Normal"}], ")"}]}], ",", "t2", ",", "0"}], "]"}], ",", "t1", 
        ",", "0"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", "0", ",", "M"}], "}"}]}], "]"}], "//", "Normal"}]}], 
  ";"}]], "Input",
