Cell[BoxData[
 RowBox[{
  RowBox[{"Ruis", "[", "f_", "]"}], ":=", 
  RowBox[{
   RowBox[{
    FractionBox[
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{
        FractionBox[
         SuperscriptBox["t", "2"], "n"], 
        SuperscriptBox["a", 
         RowBox[{"-", "2"}]]}]}], ")"}], 
     RowBox[{"1", "-", 
      SuperscriptBox["a", "2"]}]], 
    RowBox[{"(", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"l", "=", "1"}], 
        RowBox[{"2", "M"}]], 
       RowBox[{
        FractionBox["1", "l"], 
        RowBox[{"rus", "[", 
         RowBox[{
          SuperscriptBox["a", "l"], ",", 
          SuperscriptBox["t", "l"], ",", 
          SuperscriptBox["x", "l"], ",", 
          SuperscriptBox["n", "l"]}], "]"}]}]}]], ")"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"f", "/.", 
       RowBox[{"a", "->", 
        RowBox[{"a", " ", 
         SuperscriptBox["n", 
          FractionBox["1", "2"]], "x"}]}]}], "//", "PowerExpand"}], ")"}]}], 
   "+", 
   RowBox[{
    FractionBox[
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{
        FractionBox[
         SuperscriptBox["t", "2"], "n"], 
        SuperscriptBox["a", "2"]}]}], ")"}], 
     RowBox[{"1", "-", 
      SuperscriptBox["a", 
       RowBox[{"-", "2"}]]}]], 
    RowBox[{"(", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"l", "=", "1"}], 
        RowBox[{"2", "M"}]], 
       RowBox[{
        FractionBox["1", "l"], 
        RowBox[{"rus", "[", 
         RowBox[{
          SuperscriptBox["a", 
           RowBox[{"-", "l"}]], ",", 
          SuperscriptBox["t", "l"], ",", 
          SuperscriptBox["x", "l"], ",", 
          SuperscriptBox["n", "l"]}], "]"}]}]}]], ")"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"f", "/.", 
       RowBox[{"a", "->", 
        RowBox[{"a", " ", 
         SuperscriptBox["n", 
          RowBox[{"-", 
           FractionBox["1", "2"]}]], 
         SuperscriptBox["x", 
          RowBox[{"-", "1"}]]}]}]}], "//", "PowerExpand"}], 
     ")"}]}]}]}]], "Input",
