Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"GluingMeasure", "[", 
    RowBox[{"y1_", ",", "x_", ",", "n_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        RowBox[{
         SuperscriptBox["x", "3"], "n"}], 
        RowBox[{"1", "-", 
         RowBox[{
          SuperscriptBox["x", "3"], "n"}]}]], "+", 
       FractionBox[
        RowBox[{
         SuperscriptBox["x", "3"], 
         SuperscriptBox["n", 
          RowBox[{"-", "1"}]]}], 
        RowBox[{"1", "-", 
         RowBox[{
          SuperscriptBox["x", "3"], 
          SuperscriptBox["n", 
           RowBox[{"-", "1"}]]}]}]]}], ")"}]}], 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"y1", "+", 
         FractionBox["1", "y1"]}], ")"}], "2"], "-", "1"}], ")"}]}]}], 
  ";"}]], "Input",
