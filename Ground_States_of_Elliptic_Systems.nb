Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"VecSU3", "[", 
    RowBox[{"t1_", ",", "t2_", ",", "x_", ",", "n_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        RowBox[{
         SuperscriptBox["x", "3"], "n"}], 
        RowBox[{"1", "-", 
         RowBox[{
          SuperscriptBox["x", "3"], "n"}]}]], "+", 
       FractionBox[
        RowBox[{
         SuperscriptBox["x", "3"], 
         SuperscriptBox["n", 
          RowBox[{"-", "1"}]]}], 
        RowBox[{"1", "-", 
         RowBox[{
          SuperscriptBox["x", "3"], 
          SuperscriptBox["n", 
           RowBox[{"-", "1"}]]}]}]]}], ")"}]}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"t1", "+", "t2", "+", 
         FractionBox["1", 
          RowBox[{"t1", " ", "t2"}]]}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         FractionBox["1", "t1"], "+", 
         FractionBox["1", "t2"], "+", 
         RowBox[{"t1", " ", "t2"}]}], ")"}]}], "-", "1"}], ")"}]}]}], 
  ";"}]], "Input",
