   RowBox[{"sglue", "[", 
    RowBox[{"a_", ",", "x_", ",", "n_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        RowBox[{
         SuperscriptBox["x", "2"], "n"}], 
        RowBox[{"1", "-", 
         RowBox[{
          SuperscriptBox["x", "2"], "n"}]}]], "+", 
       FractionBox[
        RowBox[{
         SuperscriptBox["x", "2"], 
         SuperscriptBox["n", 
          RowBox[{"-", "1"}]]}], 
        RowBox[{"1", "-", 
         RowBox[{
          SuperscriptBox["x", "2"], 
          SuperscriptBox["n", 
           RowBox[{"-", "1"}]]}]}]]}], ")"}]}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"a", "+", 
         FractionBox["1", "a"]}], ")"}], 
       RowBox[{"(", 
        RowBox[{"a", "+", 
         FractionBox["1", "a"]}], ")"}]}], "-", "1"}], ")"}]}]}], 
  ";"}]], "Input",
