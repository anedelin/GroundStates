Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"rus", "[", 
    RowBox[{"a_", ",", "t_", ",", "x_", ",", "n_"}], "]"}], "=", 
   RowBox[{"-", 
    FractionBox[
     RowBox[{
      SuperscriptBox["x", "2"], 
      SuperscriptBox["n", 
       RowBox[{"-", "1"}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         FractionBox[
          SuperscriptBox["t", "2"], "n"], 
         SuperscriptBox["a", 
          RowBox[{"-", "2"}]]}], "+", 
        RowBox[{
         FractionBox["n", 
          SuperscriptBox["t", "2"]], 
         SuperscriptBox["a", "2"]}], "-", 
        SuperscriptBox["a", "2"], "-", 
        SuperscriptBox["a", 
         RowBox[{"-", "2"}]]}], ")"}]}], 
     RowBox[{"1", "-", 
      RowBox[{
       SuperscriptBox["x", "2"], 
       SuperscriptBox["n", 
        RowBox[{"-", "1"}]]}]}]]}]}], ";"}]], "Input",
