Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"TrinionIntegrand", "[", 
    RowBox[{
    "y1_", ",", "y2_", ",", "y3_", ",", "t_", ",", "x_", ",", "n_", ",", 
     "t1_", ",", "t2_", ",", "t3_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"Sum", "[", 
