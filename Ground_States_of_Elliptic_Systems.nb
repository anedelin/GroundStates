Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"TubeGen2", "[", 
    RowBox[{"y1_", ",", "y2_"}], "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"Series", "[", 
      RowBox[{
       RowBox[{"Coefficient", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"VandSU2", "[", "y3", "]"}], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Series", "[", 
