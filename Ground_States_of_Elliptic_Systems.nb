59e4bbe305f4"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"TubeGen1", "[", 
    RowBox[{"y1_", ",", "y4_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"Series", "[", 
     RowBox[{
      RowBox[{"Coefficient", "[", 
       RowBox[{
        RowBox[{"Coefficient", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"VandSU2", "[", "y2", "]"}], 
           RowBox[{"VandSU2", "[", "y3", "]"}], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Series", "[", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"Trinion", "[", 
                  RowBox[{"y1", ",", "y2", ",", "y3", ",", "t"}], "]"}], 
                 RowBox[{"Trinion", "[", 
                  RowBox[{"y4", ",", "y2", ",", "y3", ",", 
                   SuperscriptBox["t", 
                    RowBox[{"-", "1"}]]}], "]"}], 
                 RowBox[{"Exp", "[", 
                  RowBox[{
                   UnderoverscriptBox["\[Sum]", 
                    RowBox[{"l", "=", "1"}], 
                    RowBox[{"2", "M"}]], 
                   RowBox[{
                    FractionBox["1", "l"], 
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{"GluingMeasure", "[", 
                    RowBox[{
                    SuperscriptBox["y2", "l"], ",", 
                    SuperscriptBox["x", "l"], ",", 
                    SuperscriptBox["n", "l"]}], "]"}], "+", 
                    RowBox[{"GluingMeasure", "[", 
                    RowBox[{
                    SuperscriptBox["y3", "l"], ",", 
                    SuperscriptBox["x", "l"], ",", 
                    SuperscriptBox["n", "l"]}], "]"}]}], ")"}]}]}], "]"}]}], "//",
                 "PowerExpand"}], ",", 
               RowBox[{"{", 
                RowBox[{"x", ",", "0", ",", "M"}], "}"}]}], "]"}], "//", 
             "Normal"}], ")"}]}], ",", "y3", ",", "0"}], "]"}], ",", "y2", 
        ",", "0"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", "0", ",", "M"}], "}"}]}], "]"}], "//", "Normal"}]}], 
  ";"}]], "Input",
